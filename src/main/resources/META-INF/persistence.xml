<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ENADE_PU" transaction-type="RESOURCE_LOCAL">
    <class>br.edu.uniacademia.enade.model.Tbtipousuario</class>
    <class>br.edu.uniacademia.enade.model.Tbprova</class>
    <class>br.edu.uniacademia.enade.model.Tbquestao</class>
    <class>br.edu.uniacademia.enade.model.Tbtipoquestao</class>
    <class>br.edu.uniacademia.enade.model.Tbusuario</class>
    <class>br.edu.uniacademia.enade.model.Tbresultado</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/enade"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="org.gjt.mm.mysql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
